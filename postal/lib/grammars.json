{
    "grammars" : [
        {
            "id" : 0,
            "title" : "html",
            "filetypes" : ["html", "php"],
            "rules" : [{
                    "title": "div",
                    "type": "tagged",
                    "options" : {
                        "tagStart": "<div",
                        "namedOption" : "id=\"(.+?)\"",
                        "tagEnd": ">",
                        "closingTag": "</div>",
                        "nodeColor": "blue"
                    }
                }, {
                    "title": "href link",
                    "type" : "link",
                    "options" : {
                        "link": "href=[\"](.+?)[\"]",
                        "nodeColor": "blue"
                    }
                }, {
                    "title": "includes link",
                    "type": "link",
                    "options": {
                        "link": "include=[\"](.+?)[\"]",
                        "nodeColor": "blue"
                    }
                }, {
                    "title": "body",
                    "type": "tagged",
                    "options" : {
                        "tagStart": "<body",
                        "tagEnd": ">",
                        "closingTag": "</body>",
                        "nodeColor": "blue"
                    }
                }
            ]
        }, {
            "id" : 1,
            "title" : "javascript",
            "filetypes" : ["js"],
            "rules" : [{
                "title": "javascript requires",
                "type": "link",
                "options": {
                    "link": "requires=[\"](.+?)[\"]",
                    "nodeColor": "blue"
                }
            }]
        }, {
            "id" : 2,
            "title" : "c-like",
            "filetypes" : ["c"],
            "rules" : [{
                "title": "c-like functions",
                "type": "c-like",
                "options": {
                    "returnType": "",
                    "openParameterChar": "(",
                    "closeParameterChar": ")",
                    "openLogicChar": "{",
                    "closeLogicChar": "}"
                }
            }]
        }
    ]
}
