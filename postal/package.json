{
  "name": "postal",
  "displayName": "postal",
  "description": "typescript postal",
  "version": "0.0.1",
  "publisher": "postal",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.postal",
    "onCommand:extension.parse"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.postal",
        "title": "Postal"
      }, {
        "command": "extension.parse",
        "title": "Postal Parse"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "cwd": "^0.10.0",
    "file-system": "^2.2.1",
    "open": "0.0.5"
  }
}
